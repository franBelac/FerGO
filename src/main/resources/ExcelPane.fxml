<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="457.0" prefWidth="606.0" style="-fx-background-color: rgb(65,65,65);" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserInterface.ExcelController">
   <children>
      <Label layoutX="13.0" layoutY="14.0" text="Create your Excel sheet in Create Race first" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="24.0" />
         </font>
      </Label>
      <Label layoutX="149.0" layoutY="105.0" text="Then put the full path to it here" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Label>
      <Button fx:id="excelFinder" layoutX="224.0" layoutY="229.0" mnemonicParsing="false" onAction="#clickExcelFinder" text="Search for your excel sheet" />
      <TextField fx:id="pathField" layoutX="229.0" layoutY="308.0" />
      <Button fx:id="confirmBtn" layoutX="273.0" layoutY="374.0" mnemonicParsing="false" onAction="#clickConfirm" text="Confirm" />
      <Label fx:id="successLabel" layoutX="259.0" layoutY="269.0" text="Success!" textFill="#11ff15">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
